<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt">
    <item>
        <shape android:shape="rectangle">
            <solid android:color="#751aff"/>
            <corners android:radius="10sp"/>
        </shape>
    </item>
    <item android:right="1sp" android:bottom="2sp">
        <shape android:shape="rectangle">
            <gradient android:startColor="#944dff" android:endColor="#fff" android:angle="90"/>
            <stroke android:width="0.1dp" android:color="#751aff"/>
            <corners android:radius="10sp"/>
        </shape>
    </item>
</layer-list>
