<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt">
    <item>
        <shape android:shape="rectangle">
            <solid android:color="#bf4080"/>
            <corners android:radius="10dp"/>
        </shape>
    </item>
    <item android:right="1dp" android:bottom="2dp">
        <shape android:shape="rectangle">
            <gradient android:startColor="#cc6699" android:endColor="#ffffff" android:angle="90"/>
            <stroke android:width="0.1dp" android:color="#bf4080"/>
            <corners android:radius="10dp"/>
        </shape>
    </item>
</layer-list>
