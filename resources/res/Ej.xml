<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt">
    <item>
        <shape android:shape="rectangle">
            <solid android:color="#b3b3b3"/>
            <corners android:radius="5dp"/>
        </shape>
    </item>
    <item android:right="2dp" android:bottom="2dp">
        <shape android:shape="rectangle">
            <solid android:color="#f2f2f2"/>
            <stroke android:width="0.1dp" android:color="@color/colorPrimary"/>
            <corners android:radius="5dp"/>
        </shape>
    </item>
</layer-list>
