<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt">
    <item>
        <shape android:shape="rectangle">
            <solid android:color="@color/colorPrimary"/>
            <corners android:radius="5dp"/>
        </shape>
    </item>
    <item android:right="1sp" android:bottom="1sp">
        <shape android:shape="rectangle">
            <stroke android:width="0.5dp" android:color="@color/colorPrimary"/>
            <solid android:color="#f1edf8"/>
            <corners android:radius="5dp"/>
        </shape>
    </item>
</layer-list>
